{
  "version": 2,
  "builds": [
    {
      "src": "api/page.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/detect",
      "dest": "api/page.py"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "api/page.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 