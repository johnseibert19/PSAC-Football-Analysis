{
  "version": 2,
  "routes": [
    {
      "src": "/detect",
      "dest": "detect/page.py"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "detect/page.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "detect/**/*.py": {
      "runtime": "@vercel/python@3.12"
    }
  }
} 