{
  "version": 2,
  "routes": [
    {
      "src": "/detect",
      "dest": "detect/page.py"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "detect/page.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "public/**"
    }
  }
} 